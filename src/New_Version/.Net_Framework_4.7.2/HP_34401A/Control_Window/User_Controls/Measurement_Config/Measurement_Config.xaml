                        <Button Grid.Row="0" Grid.Column="2" Content="200"/>
                        <Button Grid.Row="0" Grid.Column="3" Content="Detector Bandwidth ?"/>
                    </Grid>
                </TabItem>
                <TabItem Header="FREQ">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
