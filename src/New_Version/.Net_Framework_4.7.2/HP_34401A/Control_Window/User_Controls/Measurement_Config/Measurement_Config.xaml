                        <Button Grid.Row="0" Grid.Column="1" Content="0.1"/>
                        <Button Grid.Row="0" Grid.Column="2" Content="1"/>
                        <Button Grid.Row="0" Grid.Column="3" Content="Frequency Aperture ?"/>
                    </Grid>
                </TabItem>
                <TabItem Header="PER">
